unsigned int array[10] = { 0x22, 0x12,0,0,0,0,0,0,0};
int main() {


  __asm__ volatile (
                "bgeu %[my_op1], %[my_op2], .L1\n"
                ".L2:\n\tadd %[my_out], %[my_op1], %[my_op2]\n"
                ".L1:\n\tavg %[my_out], %[my_op1], %[my_op2]"
                : [my_out] "=&r" (array[3])
                : [my_op1] "r" (array[0]),[my_op2] "r" (array[1])
        );

/*  __asm__ volatile (
                "avg %[my_out], %[my_op1], %[my_op2]\n"
                : [my_out] "=&r" (array[3])
                : [my_op1] "r" (array[0]),[my_op2] "r" (array[1])
        );
*/
  return 0;


}

