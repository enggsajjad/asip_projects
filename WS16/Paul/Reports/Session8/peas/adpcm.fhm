#!/usr/bin/perl
# This script generates register function definition in behavior level
# parameter : bit_width

if ($#ARGV != 0) {
    print "number of parameters is wrong.\n";
    print "usage : this_script bit_width\n";
    exit (100);
}

$bit_width    = $ARGV[0];
$msb = $bit_width - 1;

print <<FHM_DL_FOO;
/** pcmdec */
function pcmdec {
  input {
    bit [31:0] step;
    bit [31:0] delta;
    bit [31:0] valpred_in;
  }
  output {
    bit [31:0] valpred_out;
  }
  control {
    in clock_new;
    in reset_new;
    in enb_new;
  }
  protocol {
    [enb_new == '1'] {
      valid valpred_out;
    }
  }
}
FHM_DL_FOO

exit (0);
        ]]>
      </script>
