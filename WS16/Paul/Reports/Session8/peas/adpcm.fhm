  use IEEE.std_logic_unsigned.all;

entity $instance_name is
  port (
FHM_DL_TOP_2

    print <<FHM_DL_PORTS;
	clock_new    : in std_logic;
        reset_new    : in std_logic;
        enb_new      : in std_logic;
        delta  : in std_logic_vector(31 downto 0);
        step : in std_logic_vector(31 downto 0);
        valpred_in : in std_logic_vector(31 downto 0);
        valpred_out : out std_logic_vector(31 downto 0)
        );
FHM_DL_PORTS

{
print <<FHM_DL_ARCH;
end $instance_name;

architecture Behavioral of $instance_name is
$signals
begin
  $vhdl
end Behavioral;

FHM_DL_ARCH
}

exit (0);
