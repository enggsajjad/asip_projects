#!/usr/bin/perl
# This script generates register function definition in behavior level
# parameter : bit_width

if ($#ARGV != 0) {
    print "number of parameters is wrong.\n";
    print "usage : this_script bit_width\n";
    exit (100);
}

$bit_width    = $ARGV[0];
$msb = $bit_width - 1;

print <<FHM_DL_FOO;
/** clamp */
function clamp {
  input {
    bit [31:0] cmpval;
    bit [31:0] greaterval;
    bit [31:0] lesserval;
  }
  output {
    bit [31:0] dout;
  }
  control {
    in clock;
    in reset;
    in enb;
  }
  protocol {
    [enb == '1'] {
      valid dout;
    }
  }
}
FHM_DL_FOO

exit (0);
        ]]>
      </script>
