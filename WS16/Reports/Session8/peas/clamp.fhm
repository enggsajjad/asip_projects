-- Your signal declaration here
END_SIGNALS

$vhdl = <<END_VHDL;
-- Your vhdl code here
  process (clock, reset, enb)
  begin
	  if (signed(cmpval) < signed(lesserval)) then
	    dout <= lesserval;
	  elsif (signed(cmpval) > signed(greaterval)) then
	    dout <= greaterval;
	  else
	    dout <= cmpval;
	  end if;
  end process;
END_VHDL


